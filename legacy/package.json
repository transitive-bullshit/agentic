{
  "name": "agentic",
  "private": true,
  "description": "TODO",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/agentic",
  "license": "MIT",
  "type": "module",
  "source": "./src/index.ts",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.cjs",
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "del build",
    "prebuild": "run-s clean",
    "predev": "run-s clean",
    "pretest": "run-s build",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "run-p test:*",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "js-tiktoken": "^1.0.6",
    "jsonrepair": "^3.1.0",
    "ky": "^0.33.3",
    "openai-fetch": "^1.5.0",
    "p-map": "^6.0.0",
    "quick-lru": "^6.1.1",
    "ts-dedent": "^2.2.0",
    "zod": "^3.21.4",
    "zod-to-ts": "^1.1.4",
    "zod-validation-error": "^1.3.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.2.5",
    "ava": "^5.3.0",
    "del-cli": "^5.0.0",
    "dotenv": "^16.1.3",
    "expect-type": "^0.16.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "type-fest": "^3.11.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  },
  "keywords": [
    "ai",
    "agent",
    "autogpt",
    "agi",
    "babyagi",
    "llms",
    "chatgpt",
    "openai",
    "openapi",
    "guardrails",
    "plugins"
  ]
}
